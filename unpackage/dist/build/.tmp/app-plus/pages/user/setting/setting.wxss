page {
  background-color: #F4F5F7;
}
.icon {
  font-size: 30rpx;
}
.content {
  padding-bottom: 20rpx;
}
.content .list {
    width: 96%;
    padding-left: 4%;
    background-color: #fff;
    margin-bottom: 20rpx;
}
.content .list .row {
      widows: 100%;
      min-height: 90rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      border-bottom: solid 1rpx #eee;
}
.content .list .row:last-child {
        border-bottom: none;
}
.content .list .row .title {
        font-size: 32rpx;
        color: #333;
}
.content .list .row .right {
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
        color: #999;
}
.content .list .row .right .tis {
          font-size: 26rpx;
          margin-right: 5rpx;
          max-height: 120rpx;
}
.content .list .row .right .tis image {
            width: 100rpx;
            height: 100rpx;
            border-radius: 100%;
            margin: 10rpx 0;
}
.content .list .row .right .icon {
          width: 40rpx;
          color: #cecece;
}
.content .list .box {
      height: 70px;
      padding: 10px;
      background-color: #fff;
      border-bottom: 1px solid #eee;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: horizontal;
      -webkit-box-direction: normal;
      -webkit-flex-direction: row;
              flex-direction: row;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
}
.content .list .box .box_left {
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-orient: horizontal;
        -webkit-box-direction: normal;
        -webkit-flex-direction: row;
                flex-direction: row;
}
.content .list .box .box_left .content {
          padding-top: 10px;
          box-sizing: border-box;
          margin-left: 10px;
}
.content .list .box .box_left .content .rank {
            font-size: 14px;
            color: #C8C7CC;
}
.content .list .box .box_left .pic {
          width: 70px;
          height: 70px;
          border-radius: 50%;
}
.content .list .box .box_right {
        height: 80px;
}
.content .list .box .box_right .edit {
          width: 55px;
          height: 25px;
          line-height: 25px;
          color: #FFD845;
          border-radius: 5px;
          text-align: center;
          border: 1px solid #FFD845;
          margin-top: 20px;
          margin-right: 20px;
          font-size: 16px;
}
.content .list .tac {
      height: 45px;
      line-height: 45px;
      display: block;
      text-align: center;
}
.content .list .tac .title {
        color: red;
}
.addressWindows {
  width: 80%;
  background-color: #fff;
  position: fixed;
  bottom: 20%;
  left: 10%;
  padding: 15px;
  box-sizing: border-box;
  z-index: 999;
  border-radius: 5px;
}
.addressWindows .title {
    font-size: 20px;
    margin-bottom: 15px;
    text-align: center;
    margin-top: 15px;
}
.addressWindows .content {
    font-size: 16px;
}
.addressWindows .content .link {
      color: #FFD845;
      display: inline-block;
      text-decoration: underline;
}
.addressWindows .bottom {
    height: 45px;
    line-height: 45px;
    background-color: #fff;
    border-top: 1px solid #F3F3F3;
    margin-top: 20px;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    font-size: 18px;
}
.addressWindows .bottom .btn {
      width: 90px;
      text-align: center;
}
.addressWindows .bottom .color {
      color: #FFD845;
}
.windows {
  width: 100%;
  height: 300px;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 15px;
  box-sizing: border-box;
  z-index: 999;
  border-radius: 5px;
}
.windows .title {
    font-size: 22px;
    margin-bottom: 15px;
}
.windows .addressBox {
    padding: 5px;
    border-radius: 5px;
    background-color: #eeeeee;
    opacity: 0.7;
    margin-bottom: 40px;
}
.windows .ok {
    height: 30px;
    line-height: 30px;
    width: 150px;
    background-color: #ffd845;
    color: #000;
    text-align: center;
    border-radius: 5px;
    margin: 0 auto;
}
.show {
  display: block;
}
.unshow {
  display: none;
}
.dark {
  height: 100%;
  width: 100%;
  z-index: 99;
  background-color: #000;
  opacity: 0.5;
  position: fixed;
  top: 0;
  left: 0;
}

