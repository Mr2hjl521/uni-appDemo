<view><view class="content"><view class="list"><block wx:for="{{addressList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',['$0'],[[['addressList','',index]]]]]]]}}" class="row" bindtap="__e"><view class="center"><view class="name-tel"><view class="name">{{row.name}}</view><view class="tel">{{row.phone}}</view><block wx:if="{{row.isDefault==1}}"><view class="default">默认</view></block></view><view class="address">{{row.campusName+row.floorNumber+"号楼"+row.doorNumber+"宿舍"}}</view></view><view class="right"><view data-event-opts="{{[['tap',[['edit',['$0'],[[['addressList','',index,'id']]]]]]]}}" class="icon bianji" catchtap="__e"></view></view></view></block></view></view><view class="add"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="btn" bindtap="__e"><view class="icon tianjia"></view>新增地址</view></view></view>