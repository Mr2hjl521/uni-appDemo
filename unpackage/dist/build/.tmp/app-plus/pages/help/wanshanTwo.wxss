page {
  background-color: #f4f4f4;
  font-size: 16px;
  letter-spacing: 1.5px;
  box-sizing: border-box;
}
.container {
  box-sizing: border-box;
  overflow: hidden;
}
.container .box1 {
    width: 100%;
    height: 175px;
    padding-top: 20px;
    text-align: center;
    box-sizing: border-box;
    overflow: hidden;
}
.container .box1 .pic {
      width: 110px;
      height: 110px;
      border-radius: 50%;
}
.container .box1 .edit {
      color: #ffd845;
}
