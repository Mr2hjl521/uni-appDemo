}
.content .coupon .title {
      font-size: 18px;
      margin-right: 10px;
      width: 18%;
}
.content .coupon .rightPart {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: horizontal;
      -webkit-box-direction: normal;
