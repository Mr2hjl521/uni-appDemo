  padding-top: 10px;
}
.box1 .picBox {
  width: 100px;
  margin: 0 auto;
  margin-top: 30px;
}
.box1 .picBox .pic {
    width: 115px;
    height: 100px;
}
.box1 .title {
  text-align: center;
}
.content {
  height: 100px;
  padding: 0 20px;
  margin-top: 20px;
}
.content .coupon {
