page {
  background: #d6f0ff;
}
.head {
  height: 150px;
  width: 100%;
  background: red;
}
.head image {
    height: 100%;
    width: 100%;
}
.list {
  min-height: 600px;
  width: 100%;
  background: #d6f0ff;
}
.list .listson {
    height: 100px;
    width: 90%;
    background: #ffffff;
    margin: 0 5% 5% 5%;
    border-radius: 10px;
    position: relative;
}
.list .listson .title {
      font-size: 16px;
      height: 40px;
      line-height: 40px;
      width: 65%;
      border-bottom: #C8C7CC solid 1px;
      padding-left: 10%;
}
.list .listson .content {
      font-size: 14px;
      height: 50px;
      width: 70%;
      padding-left: 5%;
      color: #007AFF;
}
.list .listson .btn {
      height: 30px;
      line-height: 30px;
      width: 23%;
      background: #007AFF;
      position: absolute;
      right: 5px;
      top: 25px;
      font-size: 14px;
}

